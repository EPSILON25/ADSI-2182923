Proceso EJERCICIO2_4
	//El dueño de una tienda compra un artículo a un precio determinado. Obtener el
	//precio en que lo debe vender para obtener una ganancia del 30%.
	//Definir Variables
	Definir Producto Como Cadena;
	Definir Precio_Bruto Como Real;
	Definir Precio_Neto Como Real;
	//Asignar Variables
	Escribir "Nombre del Producto/Articulo";
	Leer Producto;
	Escribir "Precio Bruto (Antes de la ganancia)";
	Leer Precio_Bruto;
	Precio_Neto<-Precio_Bruto*1.3;
	Escribir "El precio Neto (Final) del producto ",Producto," ",Precio_Neto;
FinProceso
