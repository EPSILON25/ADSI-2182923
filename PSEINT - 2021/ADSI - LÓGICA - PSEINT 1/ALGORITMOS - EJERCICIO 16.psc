	//Calcular el área de un rectángulo de lados X e Y.
	Definir Lado_X Como Real;
	Definir Lado_Y Como Real;
	Definir area Como Real;
	
	//Asignar Variables
	Escribir "Digite el valor lado X del rectangulo: ";
	Leer Lado_X;
	Escribir "Digite el valor lado Y del rectangulo: ";
	Leer Lado_Y;
	area <- Lado_X*Lado_Y;
	Escribir "El area del rectangulo es: ",area;
