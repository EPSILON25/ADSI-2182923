	//Hacer un algoritmo que lea el nombre de una persona y número de horas que estudia en la semana.
	
	//Definir variables
	Definir Nombre Como Cadena;
	Definir horas_semana Como entero;
	
	//Asignar variables
	Escribir "Por favor escriba su nombre";
	Leer Nombre;
	Escribir "Cuantas horas estudia a la semana";
	Leer horas_semana;
